{
  "name": "netherdb",
  "version": "0.1.0",
  "description": "A lightweight, local vector database system for Obsidian notebooks with MCP support",
  "main": "dist/index.js",
  "bin": {
    "netherdb": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "npm run typecheck && npm run lint:eslint && npm run format:check",
    "lint:eslint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "obsidian",
    "vector-database",
    "mcp",
    "knowledge-base",
    "lancedb",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.21.3",
    "@modelcontextprotocol/sdk": "^1.17.3",
    "@xenova/transformers": "^2.17.2",
    "commander": "^14.0.0",
    "date-fns": "^4.1.0",
    "gray-matter": "^4.0.3",
    "marked": "^16.1.2",
    "tslib": "^2.8.1",
    "yaml": "^2.8.1",
    "zod": "^3.25.76"
  }
}
